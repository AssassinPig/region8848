<%= form_tag login_path, method: :post %>
<%= label_tag t(:username) %>
<%= text_field_tag :username,nil,placeholder:t(:username), class:'form-control' %>
<%= label_tag t(:password) %>
<%= password_field_tag :password, nil, placeholder:t(:password), class:'form-control' %>
<%= submit_tag t(:submit), class:'btn btn-default' %>
